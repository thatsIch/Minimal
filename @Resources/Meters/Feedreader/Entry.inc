MeterStyle = styleFeedReaderEntryImage
ImageName = #@#Images\Feedreader\example.jpg
LeftMouseUpAction = #HideEntry1#

[meterFeedReaderEntryTitle1]
Meter = String
MeterStyle = styleFeedReaderEntryTitle
LeftMouseUpAction = #HideEntry1#



[MeterClose]
Meter = String
X = (#NoteWidth#-12)
Y = 2
W = 11
H = 11
FontColor = 200,200,200,178
SolidColor = 0,0,0,1
FontFace = Webdings
FontSize = 7
StringAlign = Center
StringEffect = SHADOW
StringStyle = Normal
AntiAlias = 1
Text = "r"
Hidden = 1
ToolTipType = 0
ToolTipText = "Click to Close"
LeftMouseDownAction = [!WriteKeyValue "Variables" "#CURRENTCONFIG#" "inactive" "#@#Meters\Taskbar\Apps.inc"] [!Refresh "Minimal\Taskbar"] [!DeactivateConfig #CURRENTCONFIG#]
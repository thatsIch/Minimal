; Entry 1
[meterFeedReaderEntryDummy1]
Meter = Image
MeterStyle = styleFeedReaderEntryDummy|styleFeedReaderEntryDummyRow1|styleFeedReaderEntryDummyCol1

[meterFeedReaderEntryDesc1]
Meter = String
MeterStyle = styleFeedReaderEntryDescription
LeftMouseUpAction = #ShowEntry1#

[meterFeedReaderEntry1]
